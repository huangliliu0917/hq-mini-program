
.categoryTwo-container {
  width: 25%;
  display: inline-block;
  /* height: 390rpx; */
}
.categoryTwo-box {
  padding: 50rpx 0 0 45rpx;
}
.categoryTwo-item-pic image {
  width: 100rpx;
  height:100rpx;
}
.categoryTwo-item-title{
  font-size: 23rpx;
  color: #838383;
  text-align: center;
  display: block;
  margin-bottom: 40rpx;
}
.categoryTwo-goods-filter{
  
  width: 100%;
  height: 85rpx;
  background-color: #F7F7F7;
  display: flex;
  flex-direction: row;  
}
.categoryTwo-goods-filter view{
  width: 25%;
  height: 85rpx;
  line-height: 85rpx;
  text-align: center;
  font-size: 26rpx;
  color: #838383;
}
.categoryTwo-goods-filter view.active{
  color: #FE4355;
}