.index_header {
  width: 100%;
  height: 80rpx;
  position: relative;
  background-color: red;
  padding-top: 8rpx;
}

.index_search {
  height: 64rpx;
  border: 1px solid #ccc;
  margin-left: 130rpx;
  margin-right: 105rpx;
  background-color: #f6f6f6;
  line-height: 64rpx;
  text-align: center;
  border-radius: 32rpx;
}

.index_search text {
  font-size: 29rpx;
  color: #bfbfbf;
}

.header_left {
  width: 130rpx;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  font-size: 18rpx;
  line-height: 80rpx;
  text-align: center;
}

.header_right {
  width: 105rpx;
  height: 100%;
  position: absolute;
  top: 0;
  right: 0;
  line-height: 80rpx;
  text-align: center;
}
