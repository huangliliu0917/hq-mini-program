<view>
  <view class='goods-details-main-pic'>
    <image src='{{item.imgSrc}}'></image>
  </view>
  <view class='goods-details-info-box'>
    <view class='goods-item-title goods-item-logo'>
      <image src='http://jinbao.pinduoduo.com/favicon.ico'></image>
